pipeline{
  agent any
      stages {
        stage('One'){
          steps{
            echo 'Hi, this is Tyler'
            }
            }
            stage ('Two'){
              steps{
                input('Do you want to proceed?')
                }
                }
                stage('three'){
                  when{
                    not{
                      branch "master"
                      }
                      }
steps{
   echo "Hello"
   }
}
stage('Four'){
  parallel{
    stage ('Unit Test'){
      steps{
        echo "Running the unit test..."
        }
        }
        stage ('Integration Test'){
agent {
docker{
reuseNode False
image 'ubuntu'
}}
step{
echo 'Running the integration test....'
}}}
}}}}
